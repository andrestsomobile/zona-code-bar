package administracion.action;import java.io.IOException;import javax.servlet.ServletException;import java.sql.SQLException; import org.apache.struts.action.ActionMessage;  import org.apache.struts.action.ActionMessages; import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpServletResponse;import javax.servlet.http.HttpSession;import java.util.*;import administracion.control.*;import administracion.entity.*;import administracion.form.*;import org.apache.struts.action.*;  public final class empresaAction  extends Action{
			 public ActionForward execute(ActionMapping mapping,	ActionForm form, 			HttpServletRequest request,	HttpServletResponse response)			throws IOException, ServletException {
empresaForm _form = (empresaForm)form;  gstempresa control = new gstempresa(); String opcion = request.getParameter("opcion");  opcion = opcion==null || ( opcion!=null && opcion.equals("")) ? "crear" : opcion; 
//OPCION DE set:  if(opcion.equals("set")) {  empresa entity = control.getempresa(request.getParameter("codsx")); empresaForm  temp  = new empresaForm();  temp.setopcion("update"); temp.llenar(entity);  request.setAttribute("empresaForm", temp);  return mapping.findForward("empresa_datos");  }  String mensaje = "";  //opcion de CREAR  if(opcion.equals("crear")) {  String empnombre = _form.getempnombre();   String empdireccion = _form.getempdireccion();   String emptelefono = _form.getemptelefono();   String empnit = _form.getempnit();   String empdescripcion = _form.getempdescripcion();   String empmail = _form.getEmpmail(); String empslogan = _form.getEmpslogan();
  try {  control.crearempresa(empnombre,empdireccion,emptelefono,empnit,empdescripcion, empmail, empslogan);  mensaje = "empresa Creado con Exito";    } catch (SQLException e) {   e.printStackTrace();  mensaje = "No se pudo Crear el empresa: <br> " + e.getLocalizedMessage();   }  }  // Opcion de update  if(opcion.equals("update")) {   try {   control.updateempresa(_form);   mensaje = "Datos de la Empresa Actualizados con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Actualizar el empresa: <br> " + e.getLocalizedMessage();  }  }  // Opcion de eliminar  if(opcion.equals("delete")) { String codsx = request.getParameter("codsx");  try {   control.eliminar(codsx);   mensaje = "empresa Eliminado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Eliminar el empresa: <br> " + e.getLocalizedMessage();  }  }  ActionMessages e = getErrors(request);  e.add("general", new ActionMessage(mensaje, false)); addErrors(request, e);  return mapping.findForward("empresa_datos");    } }