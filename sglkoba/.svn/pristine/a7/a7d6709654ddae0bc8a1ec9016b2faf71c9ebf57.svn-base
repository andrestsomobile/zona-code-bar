package maestro.action;import java.io.IOException;import javax.servlet.ServletException;import java.sql.SQLException; import org.apache.struts.action.ActionMessage;  import org.apache.struts.action.ActionMessages; import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpServletResponse;import javax.servlet.http.HttpSession;import java.util.*;import maestro.control.*;import maestro.entity.*;import maestro.form.*;import org.apache.struts.action.*;  public final class bodegaAction  extends Action{
			 public ActionForward execute(ActionMapping mapping,	ActionForm form, 			HttpServletRequest request,	HttpServletResponse response)			throws IOException, ServletException {
bodegaForm _form = (bodegaForm)form;  gstbodega control = new gstbodega(); String opcion = request.getParameter("opcion");  opcion = opcion==null || ( opcion!=null && opcion.equals("")) ? "crear" : opcion; 
//OPCION DE set:  if(opcion.equals("set")) {  bodega entity = control.getbodega(request.getParameter("codsx")); bodegaForm  temp  = new bodegaForm();  temp.setopcion("update"); temp.llenar(entity);  request.setAttribute("bodegaForm", temp);  return mapping.findForward("bodega_datos");  }  String mensaje = "";  //opcion de CREAR  if(opcion.equals("crear")) {  String bodnombre = _form.getbodnombre();   String boddesc = _form.getboddesc();   String bodurlplano = _form.getbodurlplano();  
  try {  control.crearbodega(bodnombre,boddesc,bodurlplano);  mensaje = "bodega Creado con Exito";    } catch (SQLException e) {   e.printStackTrace();  mensaje = "No se pudo Crear el bodega: <br> " + e.getLocalizedMessage();   }  }  // Opcion de update  if(opcion.equals("update")) {   try {   control.updatebodega(_form);   mensaje = "bodega Actualizado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Actualizar el bodega: <br> " + e.getLocalizedMessage();  }  }  // Opcion de eliminar  if(opcion.equals("delete")) { String codsx = request.getParameter("codsx");  try {   control.eliminar(codsx);   mensaje = "bodega Eliminado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Eliminar el bodega: <br> " + e.getLocalizedMessage();  }  }  ActionMessages e = getErrors(request);  e.add("general", new ActionMessage(mensaje, false)); addErrors(request, e);  return mapping.findForward("bodega");    } }