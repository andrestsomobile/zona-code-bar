package maestro.action;import java.io.IOException;import javax.servlet.ServletException;import java.sql.SQLException; import org.apache.struts.action.ActionMessage;  import org.apache.struts.action.ActionMessages; import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpServletResponse;import javax.servlet.http.HttpSession;import java.util.*;import maestro.control.*;import maestro.entity.*;import maestro.form.*;import org.apache.struts.action.*;  public final class fleteAction  extends Action{
			 public ActionForward execute(ActionMapping mapping,	ActionForm form, 			HttpServletRequest request,	HttpServletResponse response)			throws IOException, ServletException {
fleteForm _form = (fleteForm)form;  gstflete control = new gstflete(); String opcion = request.getParameter("opcion");  opcion = opcion==null || ( opcion!=null && opcion.equals("")) ? "crear" : opcion; 
//OPCION DE set:  if(opcion.equals("set")) {  flete entity = control.getflete(request.getParameter("codsx")); fleteForm  temp  = new fleteForm();  temp.setopcion("update"); temp.llenar(entity);  request.setAttribute("fleteForm", temp);  return mapping.findForward("flete_datos");  }  String mensaje = "";  //opcion de CREAR  if(opcion.equals("crear")) {  String fleciudad = _form.getfleciudad();   String fletransportadora = _form.getfletransportadora();   String flevalor = _form.getflevalor();   String fledias = _form.getfledias();  
  try {  control.crearflete(fleciudad,fletransportadora,flevalor,fledias);  mensaje = "flete Creado con Exito";    } catch (SQLException e) {   e.printStackTrace();  mensaje = "No se pudo Crear el flete: <br> " + e.getLocalizedMessage();   }  }  // Opcion de update  if(opcion.equals("update")) {   try {   control.updateflete(_form);   mensaje = "flete Actualizado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Actualizar el flete: <br> " + e.getLocalizedMessage();  }  }  // Opcion de eliminar  if(opcion.equals("delete")) { String codsx = request.getParameter("codsx");  try {   control.eliminar(codsx);   mensaje = "flete Eliminado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Eliminar el flete: <br> " + e.getLocalizedMessage();  }  }  ActionMessages e = getErrors(request);  e.add("general", new ActionMessage(mensaje, false)); addErrors(request, e);  return mapping.findForward("flete");    } }