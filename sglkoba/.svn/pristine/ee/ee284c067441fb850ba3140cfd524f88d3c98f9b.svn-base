		}		// opcion de nacionalizar TODITO el embargue		if (opcion.equals("nac_todo"))			return null;		if (opcion.equals("update") && nacestado.equals("FINALIZADO")) {			int detalles = new gstnacionalizacion_detalle().getTotalReferenciasNacionalizacion(naccodsx);			if (detalles == 0) {				errors.add("nacestado", new ActionMessage("No puede finalizar un ingreso si no tiene al menos una referencia", false));				nacestado = "TRAMITE";			}		}		// valido campos requeridos:		if (nacempresa.equals(""))			errors.add("nacempresa", new ActionMessage("El nacempresa no puede ser vacio", false));		if (nacfecha.equals(""))			errors.add("nacfecha", new ActionMessage("El nacfecha no puede ser vacio", false));		if (nacingreso.equals(""))			errors.add("nacingreso", new ActionMessage("El nacingreso no puede ser vacio", false));		if (nacdeclaracion.equals(""))			errors.add("nacdeclaracion", new ActionMessage("La declaracion no puede ser vacio", false));		if (nacfechadeclaracion.equals(""))			errors.add("nacfechadeclaracion", new ActionMessage("La Fecha de la declarcion no puede ser vacio", false));		if (nacfechaingresobodega.equals(""))			errors.add("nacfechaingresobodega", new ActionMessage("La Fecha de Ingreso a bodega no puede ser vacio", false));		if (nacpaginainicial.equals(""))			errors.add("nacpaginainicial", new ActionMessage("La Pagina inicial no puede ser vacio", false));		if (nacpaginafinal.equals(""))			errors.add("nacpaginafinal", new ActionMessage("La Pagina Final no puede ser vacio", false));		if (nacconsec.equals(""))			nacconsec = null;		if (nacconseczfp.equals(""))			nacconseczfp = null;		// miro si lo va a finalizar para validar el consec y el conseczfp:		if (!naccodsx.equals("")) {			nacionalizacion nac = gnacionalizacion.getnacionalizacion(naccodsx);			if (nac != null && nacestado.equals("FINALIZADO") && nac.getnacestado().equals("TRAMITE")) {				if (nacconsec == null || nacconsec.equals("")) {					errors.add("nacconsec", new ActionMessage("El Consecutivo de Zona Franca no puede ser vacio", false));					nacestado = "TRAMITE";				}				if (nacconseczfp == null || nacconseczfp.equals("")) {					errors.add("nacconseczfp", new ActionMessage("El Numero de formulario (aprobacion ) de zona franca no puede ser vacio", false));					nacestado = "TRAMITE";				}			}		}		if (!errors.isEmpty())			errors.add("general", new ActionMessage("Existen errores, verifique la informacion", false));		return errors;	}	public void llenar(nacionalizacion entity) {		this.naccodsx = entity.getnaccodsx();		this.nacempresa = entity.getnacempresa();		this.nacfecha = entity.getnacfecha();		this.nacingreso = entity.getnacingreso();		this.nacestado = entity.getnacestado();		this.nacdeclaracion = entity.getnacdeclaracion();		this.nacfechadeclaracion = entity.getnacfechadeclaracion();		this.nacfechaingresobodega = entity.getnacfechaingresobodega();		this.nacpaginainicial = entity.getnacpaginainicial();		this.nacpaginafinal = entity.getnacpaginafinal();		this.nacdosia = entity.getnacdosia();		this.nacobservaciones = entity.getnacobservaciones();		this.nactipo = entity.getNactipo();		this.nacfob = entity.getNacfob();		this.nacpesobruto = entity.getNacpesobruto();		this.nacpesoneto = entity.getNacpesoneto();		this.nacconsec = entity.getNacconsec();		this.nacconseczfp = entity.getNacconseczfp();	}	/**	 * @return	 * @uml.property name="nacdeclaracion"	 */	public String getNacdeclaracion() {		return nacdeclaracion;	}	/**	 * @param nacdeclaracion	 * @uml.property name="nacdeclaracion"	 */	public void setNacdeclaracion(String nacdeclaracion) {		this.nacdeclaracion = nacdeclaracion;	}	/**	 * @return	 * @uml.property name="nacdosia"	 */	public String getNacdosia() {		return nacdosia;	}	/**	 * @param nacdosia	 * @uml.property name="nacdosia"	 */	public void setNacdosia(String nacdosia) {		this.nacdosia = nacdosia;	}	/**	 * @return	 * @uml.property name="nacfechadecl