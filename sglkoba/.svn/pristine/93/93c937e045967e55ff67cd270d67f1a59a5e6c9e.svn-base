package maestro.action;import java.io.IOException;import javax.servlet.ServletException;import java.sql.SQLException; import org.apache.struts.action.ActionMessage;  import org.apache.struts.action.ActionMessages; import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpServletResponse;import javax.servlet.http.HttpSession;import java.util.*;import maestro.control.*;import maestro.entity.*;import maestro.form.*;import org.apache.struts.action.*;  public final class transportadoraAction  extends Action{
			 public ActionForward execute(ActionMapping mapping,	ActionForm form, 			HttpServletRequest request,	HttpServletResponse response)			throws IOException, ServletException {
transportadoraForm _form = (transportadoraForm)form;  gsttransportadora control = new gsttransportadora(); String opcion = request.getParameter("opcion");  opcion = opcion==null || ( opcion!=null && opcion.equals("")) ? "crear" : opcion; 
//OPCION DE set:  if(opcion.equals("set")) {  transportadora entity = control.gettransportadora(request.getParameter("codsx")); transportadoraForm  temp  = new transportadoraForm();  temp.setopcion("update"); temp.llenar(entity);  request.setAttribute("transportadoraForm", temp);  return mapping.findForward("transportadora_datos");  }  String mensaje = "";  //opcion de CREAR  if(opcion.equals("crear")) {  String transpnombre = _form.gettranspnombre();   String transpdireccion = _form.gettranspdireccion();   String transptel = _form.gettransptel();   String transpmail = _form.gettranspmail();   String transpcontacto = _form.gettranspcontacto();  
  try {  control.creartransportadora(transpnombre,transpdireccion,transptel,transpmail,transpcontacto);  mensaje = "transportadora Creado con Exito";    } catch (SQLException e) {   e.printStackTrace();  mensaje = "No se pudo Crear el transportadora: <br> " + e.getLocalizedMessage();   }  }  // Opcion de update  if(opcion.equals("update")) {   try {   control.updatetransportadora(_form);   mensaje = "transportadora Actualizado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Actualizar el transportadora: <br> " + e.getLocalizedMessage();  }  }  // Opcion de eliminar  if(opcion.equals("delete")) { String codsx = request.getParameter("codsx");  try {   control.eliminar(codsx);   mensaje = "transportadora Eliminado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Eliminar el transportadora: <br> " + e.getLocalizedMessage();  }  }  ActionMessages e = getErrors(request);  e.add("general", new ActionMessage(mensaje, false)); addErrors(request, e);  return mapping.findForward("transportadora");    } }