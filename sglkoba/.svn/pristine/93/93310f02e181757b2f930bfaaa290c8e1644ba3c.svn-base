package maestro.action;import java.io.IOException;import javax.servlet.ServletException;import java.sql.SQLException; import org.apache.struts.action.ActionMessage;  import org.apache.struts.action.ActionMessages; import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpServletResponse;import javax.servlet.http.HttpSession;import java.util.*;import maestro.control.*;import maestro.entity.*;import maestro.form.*;import org.apache.struts.action.*;  public final class causal_hitAction  extends Action{
			 public ActionForward execute(ActionMapping mapping,	ActionForm form, 			HttpServletRequest request,	HttpServletResponse response)			throws IOException, ServletException {
causal_hitForm _form = (causal_hitForm)form;  gstcausal_hit control = new gstcausal_hit(); String opcion = request.getParameter("opcion");  opcion = opcion==null || ( opcion!=null && opcion.equals("")) ? "crear" : opcion; 
 String mensaje = "";  String destino = ""; //OPCION DE set:  if(opcion.equals("set")) {  causal_hit entity = control.getcausal_hit(request.getParameter("codsx")); causal_hitForm  temp  = new causal_hitForm();  temp.setopcion("update"); temp.llenar(entity);  request.setAttribute("causal_hitForm", temp);  destino = "causal_hit_datos";  }  //opcion de CREAR  if(opcion.equals("crear")) {  String cauhdesc = _form.getcauhdesc();   String cauresp = _form.getcauresp();   destino = "causal_hit"; 
  try {  control.crearcausal_hit(cauhdesc,cauresp);  mensaje = "causal_hit Creado con Exito";    } catch (SQLException e) {   e.printStackTrace();  mensaje = "No se pudo Crear el causal_hit: <br> " + e.getLocalizedMessage();   }  }  // Opcion de update  if(opcion.equals("update")) {   try {  destino = "causal_hit";   control.updatecausal_hit(_form);   mensaje = "causal_hit Actualizado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Actualizar el causal_hit: <br> " + e.getLocalizedMessage();  }  }  // Opcion de eliminar  if(opcion.equals("delete")) {  destino = "causal_hit"; String codsx = request.getParameter("codsx");  try {   control.eliminar(codsx);   mensaje = "causal_hit Eliminado con Exito";    } catch (SQLException e) { e.printStackTrace();   mensaje = "No se pudo Eliminar el causal_hit: <br> " + e.getLocalizedMessage();  }  }  ActionMessages e = getErrors(request);  e.add("general", new ActionMessage(mensaje, false)); addErrors(request, e);  return mapping.findForward(destino);    } }