          <a href="javascript:modificarPosicion(<%= temp.getentcodsx()%>,<%=ingcodsx%>,'<%=param%>','<%=pag%>')"> 
          	<img src="./disenno/images/mover.png" border="0" width="15" heigth="15" >
          </a>
          </form>
      
 <td>  <%= traf != null && traf.gettrafnumdta() != null ? traf.gettrafnumdta() : "SIN DEFINIR" %>
